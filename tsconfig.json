{
  "include": [
    "scripts/typescript/**/*.ts",
    "scripts/typescript/**/*.d.ts",
  ],
  "exclude": [],
  "compilerOptions": {
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "noImplicitAny": true,
    "outDir": "scripts",
    "sourceMap": false,
    "strict": true
  },
  "watchOptions": {
    "watchDirectory": "useFsEvents",
    "excludeDirectories": [""],
    "synchronousWatchDirectory": true
  }
}